    <asp:PlaceHolder runat="server">
        <%: Scripts.Render("~/bundles/modernizr") %>
                
    </asp:PlaceHolder>

    <script>

        $(function () {

                               $('#navigation a').stop().animate({ 'marginLeft': '-85px' }, 1000);

                               $('#navigation > li').hover(
                                   function () {
                                       $('a', $(this)).stop().animate({ 'marginLeft': '-2px' }, 200);
                                   },
                                   function () {
                                       $('a', $(this)).stop().animate({ 'marginLeft': '-85px' }, 200);
                                   }
                               );
                           

        $("#contraste").click(function () {
            //alert();
                               if ($("#contenido").hasClass("blanco-negro")) {
                                   $('#contenido').removeClass('blanco-negro');
                               } else {
                                   $('#contenido').addClass('blanco-negro');
                               }

                           });

                           var currFFZoom = 1;
                           var currIEZoom = 100;


            $('#ampliar').on('click', function () {                               
                               if ($.browser.mozilla) {
                                   var step = 0.02;
                                   currFFZoom += step;
                                   $('#contenido').css('MozTransform', 'scale(' + currFFZoom + ')');
                               } else {
                                   var step = 2;
                                   currIEZoom += step;
                                   $('#contenido').css('zoom', ' ' + currIEZoom + '%');
                               }
                           });

                           $('#reducir').on('click', function () {
                               if ($.browser.mozilla) {
                                   var step = 0.02;
                                   currFFZoom -= step;
                                   $('#contenido').css('MozTransform', 'scale(' + currFFZoom + ')');

                               } else {
                                   var step = 2;
                                   currIEZoom -= step;
                                   $('#contenido').css('zoom', ' ' + currIEZoom + '%');
                               }
            });
        });

    </script>

    <webopt:bundlereference runat="server" path="~/Content/css" />
