    <style>
      .nav > li > a:hover, .nav > li > a:focus{ 
        background-color:#c80000 !important;            
      }
          .blanco-negro {
            -webkit-filter: grayscale(100%);
            filter: grayscale(100%);
          }
          ul#navigation {
              position: fixed;margin: 0px;padding: 0px;top: 10px;left: 0px;list-style: none;z-index: 9999;
          }
          ul#navigation li {width:100px;}

          ul#navigation li a {display: block;margin-left: -85px;width: 100px;height: 70px;background-color: #aaa;background-repeat: no-repeat;background-position: center center;border: 1px solid #AFAFAF;}

          ul#navigation li a {display: block;margin-left: -85px;width: 100px;height: 70px;background-color: #aaa;background-repeat: no-repeat;background-position: center center;border: 1px solid #AFAFAF;-moz-border-radius: 0px 10px 10px 0px;-webkit-border-bottom-right-radius: 10px;-webkit-border-top-right-radius: 10px;-khtml-border-bottom-right-radius: 10px;-khtml-border-top-right-radius: 10px;opacity: 0.6;filter: progid:DXImageTransform.Microsoft.Alpha(opacity=60);}
    
          ul#navigation .home a {background-image: url(img/home.svg);}

          ul#navigation .ampliar a {background-image: url(img/zoom_in_magnifier.svg);}

          ul#navigation .reducir a {background-image: url(img/zoom_out_magnifier.svg);}
          ul#navigation .contraste a {background-image:url(img/promotion.svg);}
          ul#navigation .Encuesta a {background-image:url(img/encuesta.svg);background-size: 60px 60px;}
          ul#navigation .Manual a {background-image:url(img/manual.png);background-size: 60px 60px;}

         
          ul#navigation .rssfeed a {background-image: url(../images/rss.png);}

          ul#navigation .photos a {background-image: url(../images/camera.png);}

          ul#navigation .contact a {background-image: url(../images/mail.png);}

      </style>
